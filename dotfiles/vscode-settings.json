{
  "workbench.colorTheme": "Solarized Light",
  "git.enableSmartCommit": true,
  "editor.rulers": [79, 87, 119],
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "vim.commandLineModeKeyBindings": [],
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-v>": false,
    "<C-w>": false,
    "<C-n>": false,
    "<C-f>": false,
    "<C-d>": false,
    "<C-x>": false,
    "<tab>": false
  },
  "vim.normalModeKeyBindings": [
    {
      "before": ["Y"],
      "after": ["y", "$"]
    },
    {
      "before": ["shift+tab"],
      "after": ["<"]
    },
    {
      "before": ["tab"],
      "after": [">"]
    }
  ],
  // "editor.lineNumbers": "relative",
  "editor.tabCompletion": true,
  "editor.snippetSuggestions": "top",
  "python.showStartPage": false,
  "firefox.executable": "/usr/bin/firefox",
  "firefox.profile": "vscode-debug",
  "debug.javascript.autoExpandGetters": true,
  "firefox.keepProfileChanges": true,
  "workbench.colorCustomizations": {},
  "remote.extensionKind": {
    "ms-azuretools.vscode-docker": "workspace"
  },
  "git.autofetch": true,
  "python.linting.flake8Enabled": true,
  "terminal.integrated.env.linux": {
    "EDITOR": "code --wait"
  },
  "terminal.integrated.allowChords": false,
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.experimentalLinkProvider": false,
  "python.experiments.optOutFrom": ["All"],
  "cSpell.enableFiletypes": ["shellscript"],
  "todo-tree.tree.showScanModeButton": false,
  "git.allowNoVerifyCommit": true,
  "cSpell.userWords": ["gitlab", "odoo", "pylint", "tecnativa"],
  "window.titleBarStyle": "custom",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "gitlens.gitCommands.skipConfirmations": ["fetch:command", "switch:command"],
  "githubPullRequests.remotes": ["origin", "upstream", "Tecnativa"],
  "python.languageServer": "Jedi",
  "gitlab.instanceUrl": "https://gitlab.tecnativa.com",
  "terminal.external.linuxExec": "gome-terminal",
  "terminal.integrated.scrollback": 1000000,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.autocomplete.enable": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
  },
  "vim.smartRelativeLine": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "prettier.endOfLine": "auto",
  "terminal.integrated.fontFamily": "MesloLGS NF"
}
